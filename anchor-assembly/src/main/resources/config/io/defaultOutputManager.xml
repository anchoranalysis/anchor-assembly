<config>
<bean config-class="org.anchoranalysis.io.output.bean.OutputManagerPermissiveExcept" delExistingFolder="false">
    
    <filePathPrefixer config-class="org.anchoranalysis.plugin.io.bean.filepath.prefixer.LastDirectoryAsPrefix">
        <filePathPrefixer config-class="org.anchoranalysis.plugin.io.bean.filepath.prefixer.FromDescriptiveName"/>
    </filePathPrefixer>

    <except config-class="org.anchoranalysis.bean.StringSet" config-factory="stringSet">
      <item>manifest</item>
      <item>manifestExperiment</item>
      <item>config</item>
      <item>executionTime</item>
    </except>
    
    <exceptSecondLevel config-class="java.util.List" config-factory="list">
    
        <item config-class="org.anchoranalysis.bean.NamedBean" name="stack">
		    <item config-class="org.anchoranalysis.bean.StringSet" config-factory="stringSet">
		      <item>input_image</item>
		    </item>
        </item>
    </exceptSecondLevel>
</bean>
</config>