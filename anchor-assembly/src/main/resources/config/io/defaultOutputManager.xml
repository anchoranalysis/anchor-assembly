<config>
<bean config-class="org.anchoranalysis.io.output.bean.OutputManager" silentlyDeleteExisting="false">
    
    <prefixer config-class="org.anchoranalysis.plugin.io.bean.filepath.prefixer.IfIncrementingNumberRequested">
    	<prefixerIncrementingNumber numberDigits="4" config-class="org.anchoranalysis.io.output.bean.path.prefixer.IncrementingNumber"/>
	    <prefixerElse config-class="org.anchoranalysis.plugin.io.bean.filepath.prefixer.LastDirectoryAsPrefix">
	        <prefixer config-class="org.anchoranalysis.plugin.io.bean.filepath.prefixer.FromDescriptiveName"/>
	    </prefixerElse>
    </prefixer>

</bean>
</config>