<!-- Converts an image into default output format (usually TIFF) -->
<config>
<bean config-class="org.anchoranalysis.plugin.image.task.bean.FormatConverterTask">
    <chnlConversionStyle config-class="org.anchoranalysis.plugin.image.task.bean.chnl.conversionstyle.RGB">
        <fallback config-class="org.anchoranalysis.plugin.image.task.bean.chnl.conversionstyle.IndependentChnls"/>
    </chnlConversionStyle>
</bean>
</config>
