<!-- Converts an image into default output format (usually TIFF) -->
<config>
<bean config-class="org.anchoranalysis.plugin.image.task.bean.format.FormatConverterTask">
    <channelConversionStyle config-class="org.anchoranalysis.plugin.image.task.bean.format.convert.RGB">
        <fallback config-class="org.anchoranalysis.plugin.image.task.bean.format.convert.IndependentChannels"/>
    </channelConversionStyle>
</bean>
</config>
