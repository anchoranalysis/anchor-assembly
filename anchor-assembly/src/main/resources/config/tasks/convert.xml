<!-- Converts an image into default output format (usually TIFF) -->
<config>
<bean config-class="org.anchoranalysis.plugin.image.task.bean.format.FormatConverterTask">
    <chnlConversionStyle config-class="org.anchoranalysis.plugin.image.task.bean.format.convert.RGB">
        <fallback config-class="org.anchoranalysis.plugin.image.task.bean.format.convert.IndependentChnls"/>
    </chnlConversionStyle>
</bean>
</config>
