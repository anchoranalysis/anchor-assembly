 <config>
 <bean config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskList">
    <list config-class="java.util.List" config-factory="list">
                
    <!--  Export Graph NRG Time Series -->
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" incrementSize="1000" outputName="nrgBreakdownTime">
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.PlotEnergy" width="500" height="300">
                <graphDefinition config-class="org.anchoranalysis.gui.plot.bean.BarPlotEnergyBreakdown" showDomainAxis="false"/>
            </createRasterGenerator>
        </item>
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" incrementSize="1000" outputName="textCfgSize">
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.NumberMarksAsText">
                <stringGenerator config-class="org.anchoranalysis.image.io.bean.generator.StringRasterGenerator" fontSize="36">
                	<size width="200" height="100" config-class="org.anchoranalysis.image.bean.spatial.SizeXY"/>
                </stringGenerator>
            </createRasterGenerator>
        </item>
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskCSVStatistic" incrementSize="1000" outputName="dynamicCfgSize">
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.csvstatistic.PlotNumberMarks" width="500" height="300">
                <graphDefinition config-class="org.anchoranalysis.gui.plot.definition.line.LinePlotNumberMarks"/>
            </createRasterGenerator>
        </item>
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskCSVStatistic" incrementSize="1000" outputName="dynamicNRG">
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.csvstatistic.PlotEnergy" width="500" height="300">
                <graphDefinition config-class="org.anchoranalysis.gui.plot.definition.line.LinePlotIterationVsEnergy"/>
            </createRasterGenerator>

    </item>

    <!-- increment size previously 1000 -->
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" incrementSize="1" outputName="rgbOutline">
            <createRasterGenerator index="0" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                <item backgroundStackName="nrgChnl-0" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects"/>
            </createRasterGenerator>
        </item>
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMergedMarks" incrementSize="1" outputName="rgbOutlineMerged">
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects" mip="true" colorFromIter="true"/>
        </item>
        
        
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" incrementSize="1000" startAtEnd="true" outputName="rgbOutlineBackward">
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects" mip="true"/>
        </item>
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" incrementSize="1" startAtEnd="true" outputName="rgbOutlineLast">
            <createRasterGenerator backgroundStackName="nrgChnl-0" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects"/>
        </item>
                
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" outputName="combined" incrementSize="1">
                
                    <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.CombinedMarks">
                    
                        <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="2" numCols="2">
                            <cells config-class="java.util.List" config-factory="arrangeRasterCellList">
                                <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="0" col="0">
                                    <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterOverlay" horizontalAlign="left" verticalAlign="top"/>
                                </item>
                                <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="0" col="1">
                                    <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="2" numCols="1"/>
                                </item>
                            </cells>
                        </arrange>
                        
                        <createRasterGeneratorList config-class="java.util.List" config-factory="list">
                            <item backgroundStackName="nrgChnl-0" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects"/>
        
                            <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.NumberMarksAsText">
                                <stringGenerator config-class="org.anchoranalysis.image.io.bean.generator.StringRasterGenerator" fontSize="28" fontName="SansSerif" bold="true">
                                    <size width="45" height="50" config-class="org.anchoranalysis.image.bean.spatial.SizeXY"/>
                                    <fontColor config-class="org.anchoranalysis.io.bean.color.RGBColorBean" red="255" green="255" blue="255"/>
                                </stringGenerator>
                            </item>
                                                
                            <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.dynamically.PlotNumberMarks" width="400" height="200">
                                <graphDefinition config-class="org.anchoranalysis.gui.plot.definition.line.LinePlotNumberMarks">
                                    <graphColorScheme config-class="org.anchoranalysis.plot.bean.colorscheme.BlackBackgroudGraphColorScheme"/>
                                </graphDefinition>
                            </item>
                            <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.dynamically.PlotNumberMarks" width="400" height="200">
                                <graphDefinition config-class="org.anchoranalysis.gui.plot.definition.line.LinePlotIterationVsEnergy">
                                    <graphColorScheme config-class="org.anchoranalysis.plot.bean.colorscheme.BlackBackgroudGraphColorScheme"/>
                                </graphDefinition>
                            </item>
                        </createRasterGeneratorList>

            </createRasterGenerator>
        </item>
        
        
        
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" outputName="combinedCornea" incrementSize="10">
                
                    <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.Combined">
                    
                        <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="2" numCols="2">
                            <cells config-class="java.util.List" config-factory="arrangeRasterCellList">
                                <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="0" col="0">
                                    <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterOverlay" horizontalAlign="left" verticalAlign="top"/>
                                </item>
                                <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="0" col="1">
                                    <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterOverlay" horizontalAlign="left" verticalAlign="top"/>
                                </item>
                                <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="1" col="0">
                                    <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="1" numCols="1"/>
                                </item>
                                <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="1" col="1">
                                    <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterOverlay" horizontalAlign="left" verticalAlign="top"/>
                                </item>
                            </cells>
                        </arrange>
                        
                        <createRasterGeneratorList config-class="java.util.List" config-factory="list">
                        
                            <item index="2" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                                <item backgroundStackName="nrgChnl-0" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects"/>
                            </item>
                            
                            <item index="2" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                                <item prefix="Proposed Segments: size=" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.NumberMarksAsText">
                                    <stringGenerator config-class="org.anchoranalysis.image.io.bean.generator.StringRasterGenerator" fontSize="28" fontName="SansSerif" bold="true">
                                        <size width="500" height="50" config-class="org.anchoranalysis.image.bean.spatial.SizeXY"/>
                                        <fontColor config-class="org.anchoranalysis.io.bean.color.RGBColorBean" red="0" green="255" blue="255"/>
                                    </stringGenerator>
                                </item>
                            </item>
                            
                            <item index="1" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                                <item backgroundStackName="nrgChnl-0" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects"/>
                            </item>

                            
                            <item index="2" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                                <item text="Proposed Ellipse" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.ConstantStringRasterGenerator">
                                    <stringGenerator config-class="org.anchoranalysis.image.io.bean.generator.StringRasterGenerator" fontSize="28" fontName="SansSerif" bold="true">
                                        <size width="500" height="50" config-class="org.anchoranalysis.image.bean.spatial.SizeXY"/>
                                        <fontColor config-class="org.anchoranalysis.io.bean.color.RGBColorBean" red="0" green="255" blue="255"/>
                                    </stringGenerator>
                                </item>
                            </item>

                            <item index="1" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                                <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.dynamically.PlotNumberMarks" width="700" height="400">
                                    <graphDefinition config-class="org.anchoranalysis.gui.plot.definition.line.LinePlotIterationVsEnergy" minMaxIgnoreBeforeIndex="100">
                                        <graphColorScheme config-class="org.anchoranalysis.plot.bean.colorscheme.BlackBackgroudGraphColorScheme"/>
                                    </graphDefinition>
                                </item>
                            </item>

                            <item index="0" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                                <item backgroundStackName="nrgChnl-0" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects"/>
                            </item>
                            
                                                        
                            <item index="2" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.DemuxDualState">
                                <item text="Best-Fitting Ellipse" config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.ConstantStringRasterGenerator">
                                    <stringGenerator config-class="org.anchoranalysis.image.io.bean.generator.StringRasterGenerator" fontSize="28" fontName="SansSerif" bold="true">
                                    	<size width="500" height="50" config-class="org.anchoranalysis.image.bean.spatial.SizeXY"/>
                                        <fontColor config-class="org.anchoranalysis.io.bean.color.RGBColorBean" red="0" green="255" blue="255"/>
                                    </stringGenerator>
                                </item>
                            </item>

                        </createRasterGeneratorList>

            </createRasterGenerator>
        </item>
        
        
        
        <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" outputName="combinedNuc" incrementSize="100">
        
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.CombinedMarks">
            
                <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="2" numCols="2">
                    <cells config-class="java.util.List" config-factory="arrangeRasterCellList">
                    <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="0" col="0">
                            <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="2" numCols="1"/>
                        </item>

                    </cells>
                </arrange>
                
                <createRasterGeneratorList config-class="java.util.List" config-factory="list">
                    <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects" mip="true"/>

                    <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.NumberMarksAsText">
                        <stringGenerator config-class="org.anchoranalysis.image.io.bean.generator.StringRasterGenerator" fontSize="28" fontName="SansSerif" bold="true">
                            <size width="70" height="100" config-class="org.anchoranalysis.image.bean.spatial.SizeXY"/>
                            <fontColor config-class="org.anchoranalysis.io.bean.color.RGBColorBean" red="255" green="255" blue="255"/>
                        </stringGenerator>
                    </item>
                </createRasterGeneratorList>
            </createRasterGenerator>
    </item>
    
    
    
        <item outputName="combinedNuc2" config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" incrementSize="1000" startAtEnd="true" limitIterations="1">
            <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects" mip="true"/>
        </item>
                
                <item config-class="org.anchoranalysis.plugin.gui.bean.exporttask.ExportTaskMarks" outputName="combined2" incrementSize="100">
                
                    <createRasterGenerator config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.CombinedMarks">
                    
                        <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="1" numCols="2">
                            <cells config-class="java.util.List" config-factory="arrangeRasterCellList">
                                <item config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterCell" row="0" col="1">
                                    <arrange config-class="org.anchoranalysis.image.bean.arrangeraster.ArrangeRasterTile" numRows="2" numCols="1"/>
                                </item>
                            </cells>
                        </arrange>
                        
                        <createRasterGeneratorList config-class="java.util.List" config-factory="list">
                            <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.DrawObjects" mip="true"/>
        

                            <item config-class="org.anchoranalysis.plugin.gui.bean.createrastergenerator.energybreakdown.dynamically.PlotNumberMarks" width="400" height="200">
                                <graphDefinition config-class="org.anchoranalysis.gui.plot.definition.line.LinePlotIterationVsEnergy">
                                    <graphColorScheme config-class="org.anchoranalysis.plot.bean.colorscheme.BlackBackgroudGraphColorScheme"/>
                                </graphDefinition>
                            </item>
                        </createRasterGeneratorList>
                </createRasterGenerator>
        </item>
      </list>   
  </bean>
  </config>
  